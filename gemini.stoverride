#!desc= gemini规则
name: gemini-rules
desc: gemini规则


#美国节点代理
proxy-groups:
  - name: USA
    type: select
    include-all: true
    filter: '美' 
rule-providers:
  gemini:
    behavior: classical
    format: yaml
    url: "https://raw.githubusercontent.com/qiudxx/stash-rule/refs/heads/main/Gemini.yaml"
    interval: 864000

rules:
  - RULE-SET,gemini,USA

